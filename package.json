  "dependencies": {
    "async-mutex": "^0.5.0",
    "bull": "^4.16.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "joi": "^18.0.1",
    "mkcert": "^3.2.0",
    "mysql2": "^3.15.3",
    "redis": "^5.8.3",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0"
